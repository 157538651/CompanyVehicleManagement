<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="model">

	<class name="Application" table="application">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="user_id"></property>
		<property name="driver_id"></property>
		<property name="car_id"></property>
		<property name="beginday"></property>
		<property name="endday"></property>
		<property name="setday"></property>
		<property name="state"></property>
		<property name="doneday"></property>
		<property name="reason"></property>

	</class>

</hibernate-mapping>